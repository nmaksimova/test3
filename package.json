    "serve": "gatsby serve",
    "develop": "gatsby develop --port 8002 --host 0.0.0.0",
    "start": "npm run develop",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint --ext js --ext jsx ."
